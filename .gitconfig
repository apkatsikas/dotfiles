[push]
	default = current
[alias]
	alog = !git log --graph --abbrev-commit
	la = !git config --list | grep -F alias
	stat=!git status
	# Show branches sorted by last commit and include the date in the output
	branchl=!git for-each-ref --sort=authordate --format '%(authordate:iso) * %(color:green)%(refname:short)%(color:black) %(subject)' --sort=-committerdate refs/heads
[color]
	ui = auto
