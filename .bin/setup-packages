#!/usr/bin/env bash

echo 'Install a bunch of basic packages...'
sudo apt-get install \
  curl \
  gnuplot-x11 \
  htop \
  jq \
  libgimp2.0-dev \
  links \
  moreutils \
  mosh \
  ranger \
  screen \
  texlive-fonts-recommended \
  texlive-latex-recommended \
  vim \
  w3m \
  x11-utils \
  xclip

